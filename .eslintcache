archBar';\r\nimport StoriesList from '../components/StoriesList';\r\nimport Pagination from '../components/Pagination';\r\nimport Loader from '../components/Loader';\r\n\r\nexport default function Stories(props){\r\n\r\n    //Verifying localStorage\r\n    if(!localStorage.getItem('storiesFilters')){\r\n        localStorage.setItem('storiesFilters', JSON.stringify({\r\n            stories: [],\r\n            offset: (props.match.params.page - 1) * 30,\r\n            characterID: '',\r\n            comicID: '',\r\n            limit: 30,\r\n            totalResults: 0,\r\n            currentPage: props.match.params.page\r\n        }))\r\n    }\r\n\r\n    //states\r\n    const [load, setLoad] = useState(true);\r\n    const [info, setInfo] = useState(JSON.parse(localStorage.getItem('storiesFilters')));\r\n    const {offset, characterID, comicID, limit, totalResults, currentPage} = info;\r\n\r\n    //Getting stories information\r\n    useEffect(()=>{\r\n        document.documentElement.scrollTop = 0;\r\n        loadCharacters();\r\n    },[characterID, comicID])\r\n\r\n    async function loadCharacters(){\r\n        setLoad(true);\r\n\r\n        //Generate url strings according to filters\r\n        const mainStructure = `https://gateway.marvel.com:443/v1/public/stories?&limit=${limit}&`;\r\n\r\n        let url = mainStructure;\r\n        if(offset){url += `offset=${offset}&`}\r\n        if(characterID){ url += `characters=${characterID}&` }\r\n        if(comicID){url += `comics=${comicID}&`}\r\n\r\n        url += 'ts=1&apikey=6cd68a4b84b42955f0f3bb84dec3f553&hash=b0bb162537de4f6bbebf28a7ac757197';\r\n\r\n        //Request info to the api with the complete url string\r\n        try{\r\n            const {data: {data}} = await axios.get(url);\r\n\r\n            await setInfo({\r\n                ...info,\r\n                stories: data.results,\r\n                totalResults: data.total\r\n            })\r\n            setLoad(false);\r\n        } catch(err){\r\n            console.log(err);\r\n        }\r\n\r\n        //Saving localStorage info for the filters\r\n        localStorage.setItem('storiesFilters', JSON.stringify(info))\r\n    }\r\n\r\n    return(\r\n        <>\r\n            <Helmet>\r\n                <title>Marvel Wiki - Stories</title>\r\n            </Helmet>\r\n\r\n            <Header title1=\"marvel\" title2=\"Stories\" img=\"../img/stories.jpg\"/>\r\n            <StoriesContext.Provider value={{info, setInfo}}>\r\n                <StoriesSearchBar/>\r\n                {load ? \r\n                    <Loader/> \r\n                    :\r\n                    <StoriesList/>\r\n                }\r\n                {totalResults > 0 && <Pagination url=\"stories\"/>}\r\n            </StoriesContext.Provider>\r\n        </>\r\n    )\r\n}","C:\\Proyectos\\Marvel-Wiki\\src\\components\\MobileMenu.jsx",[],"C:\\Proyectos\\Marvel-Wiki\\src\\context\\CharactersContext.js",[],"C:\\Proyectos\\Marvel-Wiki\\src\\pages\\Characters.jsx",["179"],"C:\\Proyectos\\Marvel-Wiki\\src\\index.js",[],"C:\\Proyectos\\Marvel-Wiki\\src\\pages\\Comics.jsx",["180","181"],"C:\\Proyectos\\Marvel-Wiki\\src\\context\\StoriesContext.js",[],"C:\\Proyectos\\Marvel-Wiki\\src\\components\\Header.jsx",[],"C:\\Proyectos\\Marvel-Wiki\\src\\components\\Loader.jsx",[],"C:\\Proyectos\\Marvel-Wiki\\src\\pages\\Comic.jsx",["182"],"C:\\Proyectos\\Marvel-Wiki\\src\\components\\Pagination.jsx",["183","184","185","186"],{"ruleId":"187","severity":1,"message":"188","line":37,"column":7,"nodeType":"189","endLine":37,"endColumn":55,"suggestions":"190"},{"ruleId":"191","severity":1,"message":"192","line":10,"column":9,"nodeType":"193","messageId":"194","endLine":20,"endColumn":10},{"ruleId":"195","severity":1,"message":"196","line":13,"column":17,"nodeType":"197","messageId":"198","endLine":13,"endColumn":23},{"ruleId":"195","severity":1,"message":"196","line":16,"column":17,"nodeType":"197","messageId":"198","endLine":16,"endColumn":23},{"ruleId":"195","severity":1,"me